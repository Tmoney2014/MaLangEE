# Environment Variables Template
# Copy this file to .env.local and fill in your values
# cp .env.example .env.local

# Figma API Token
# Get from: Figma Settings > Account > Personal access tokens
FIGMA_API_KEY=

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPEN_AI_API=

# 로컬호스트 URL
# 개발 환경에서 사용
NEXT_PUBLIC_LOCALHOST_URL=http://localhost:8000

# Backend API URL (기본 경로: /api/v1 자동 포함)
# 예: http://localhost:8000 => API 호출 시 http://localhost:8000/api/v1/... 사용
NEXT_PUBLIC_API_URL=http://49.50.137.35:8080

